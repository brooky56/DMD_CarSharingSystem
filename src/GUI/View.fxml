<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" scaleShape="false" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="70.0" prefWidth="421.0" AnchorPane.bottomAnchor="317.0" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0">
         <children>
            <Label fx:id="queryLabel" prefHeight="14.0" prefWidth="215.0" text="Query field:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="queryField" prefHeight="27.0" prefWidth="421.0" />
            <Button fx:id="executeButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="127.0" text="Execute query" />
         </children>
      </VBox>
      <TableView fx:id="table" layoutX="14.0" layoutY="90.0" prefHeight="303.0" prefWidth="572.0" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="90.0" />
      <ComboBox fx:id="dbTableBox" editable="true" layoutX="436.0" layoutY="33.0" prefHeight="27.0" prefWidth="150.0" promptText="Choose  the table" visibleRowCount="20" />
      <Button fx:id="buttonShowTable" layoutX="435.0" layoutY="60.0" mnemonicParsing="false" onAction="#onButtonShowClick" prefHeight="25.0" prefWidth="82.0" text="Show table" />
      <Button fx:id="buttonClearTableView" layoutX="518.0" layoutY="60.0" mnemonicParsing="false" onAction="#onButtonClearClick" prefHeight="25.0" prefWidth="68.0" text="Clear" />
   </children>
</AnchorPane>
