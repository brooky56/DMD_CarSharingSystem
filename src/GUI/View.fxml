<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" scaleShape="false" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller">
    <children>
        <TableView fx:id="tableUsers" layoutX="14.0" layoutY="50.0" prefHeight="200.0" prefWidth="571.0" AnchorPane.bottomAnchor="150.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="50.0">
            <columns>
                <!-- Колонки таблицы -->
                <TableColumn fx:id="idColumn" prefWidth="80.0" text="№" />
                <TableColumn fx:id="loginColumn" prefWidth="150.0" text="Login" />
                <TableColumn fx:id="passwordColumn" prefWidth="150.0" text="Password" />
                <TableColumn fx:id="emailColumn" prefWidth="220.0" text="Email" />
            </columns>
        </TableView>
        <Button fx:id="buttonClick" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onButtonClick" text="Click" />
    </children>
</AnchorPane>
